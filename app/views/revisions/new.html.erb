<%= form_for(@revision, :html => { :multipart => true }) do |f| %>
  <%= render :partial => "shared/form_error_messages", :locals => { :target => @revision } %>
  <%= f.hidden_field :document_id %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>